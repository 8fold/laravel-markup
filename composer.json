{
  "name": "8fold/laravel-uikit",
  "description": "A library for implementing HTML UI components in Laravel projects.",
  "type": "library",
  "license": "MIT",
  "version": "0.0.1",
  "authors": [
    {
      "name": "Josh Bruce",
      "email": "josh@joshuabruce.com"
    }
  ],
  "repositories": [
    {
      "type": "path",
      "url": "../php-uikit",
      "name": "8fold/php-uikit"
    }
  ],
  "require": {
    "php": "~7",
    "8fold/php-uikit": "~0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.1"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Eightfold\\LaravelUIKit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eightfold\\LaravelUIKit\\Tests\\": "tests/"
    }
  }
}
